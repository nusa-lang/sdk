muat 'cetak.n'

/*
    Declare a variable

    Syntax:
    type variablename
    type variablename = value
*/

teks name
bul age

// Assigning a value to a variable.
name = 'Aerell'
age = 20

cetak(name + '\n')
cetak((teks)age + '\n')

// Definition a variable
teks name2 = 'Aerell'
cetak(name2 + '\n')